<div id="pages-widget" class="proton-widget messages lit">
    <div class="panel panel-default back">
        <div class="panel-heading">
            <i class="icon-cog"></i>
            <span>Settings</span>
            <div class="toggle-widget-setup">
                <i class="icon-ok"></i>
                <span>DONE</span>
            </div>
        </div>
        <ul class="list-group">
            <li class="list-group-item">
                <div class="form-group">
                    <label>Filter by Location</label>
                    <div>
                        <div id="s2id_autogen1" class="select2-container select2"><a href="javascript:void(0)" onclick="return false;" class="select2-choice" tabindex="-1">   <span class="select2-chosen">Any</span><abbr class="select2-search-choice-close"></abbr>   <span class="select2-arrow"><b></b></span></a><input id="s2id_autogen2" class="select2-focusser select2-offscreen" type="text"><div class="select2-drop select2-display-none select2-with-searchbox">   <div class="select2-search">       <input autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" class="select2-input" type="text">   </div>   <ul class="select2-results">   </ul></div></div><select tabindex="-1" class="select2 select2-offscreen">
                            <option selected="selected" value="Any">Any</option>
                            <option value="Europe">Europe</option>
                            <option value="Asia">Asia</option>
                            <option value="North America">North America</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="form-group">
                    <label>Filter by Time</label>
                    <div>
                        <div id="s2id_autogen3" class="select2-container select2"><a href="javascript:void(0)" onclick="return false;" class="select2-choice" tabindex="-1">   <span class="select2-chosen">This Week</span><abbr class="select2-search-choice-close"></abbr>   <span class="select2-arrow"><b></b></span></a><input id="s2id_autogen4" class="select2-focusser select2-offscreen" type="text"><div class="select2-drop select2-display-none select2-with-searchbox">   <div class="select2-search">       <input autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" class="select2-input" type="text">   </div>   <ul class="select2-results">   </ul></div></div><select tabindex="-1" class="select2 select2-offscreen">
                            <option>Any</option>
                            <option>Last Hour</option>
                            <option>Today</option>
                            <option selected="selected">This Week</option>
                            <option>This Month</option>
                            <option>This Year</option>
                        </select>
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <div class="panel panel-success front">
        <div class="panel-heading">
            <i class="icon-folder-open"></i>
            <span>Pages</span>
            <i class="icon-cog toggle-widget-setup"></i>
        </div>
        <div>
            <ul class="list-group pending">
                {% if pages is defined and pages|length %}
                    {% for page in pages %}
                        <li class="list-group-item">
                            <i class="icon-folder-open">{#<img src="Proton%20UI%20-%20Dashboard_files/user1.jpg" alt="User Icon">#}</i>
                            <div class="text-holder">
                                <span class="title-text">
                                    {{ page.title }}
                                </span>
                                <span class="description-text">
                                    test
                                </span>
                            </div>
                            <span class="stat-value">
                                
                            </span>
                        </li>
                    {% endfor %}
                {% endif %}
            </ul>
        </div>
    </div>
</div>