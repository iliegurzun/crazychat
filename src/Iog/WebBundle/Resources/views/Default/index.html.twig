{% extends "IogWebBundle::layout.html.twig" %}
{% block title %}
    {{ page.title }}
{% endblock  %}

{% block content %}

    {% for block in page.blocks %}

        
        <img src="{{ qrcode_url(block.content|default()|striptags) }}" />
        <img src="{{ qrcode_url(block.content|default()|raw, 'png', 150) }}" />
        {{ block.content|default()|raw }}

    {% endfor %}
{% endblock content %}